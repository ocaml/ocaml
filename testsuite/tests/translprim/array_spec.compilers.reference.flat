          eta_gen_unsafe_get eta_gen_safe_set eta_gen_unsafe_set eta_int_len
          eta_int_safe_get eta_int_unsafe_get eta_int_safe_set
          eta_int_unsafe_set eta_float_len eta_float_safe_get
